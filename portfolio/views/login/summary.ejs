<!DOCTYPE html>
<html class="no-js">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <title>My Profile</title>
  <meta name="description" content="">
  <meta name="viewport" content="width=device-width">
  <link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css" rel="stylesheet">
  <link rel="stylesheet" href="css/normalize.min.css">
  <link rel="stylesheet" href="css/main.css">
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.1/jquery.min.js"></script>
  <script>window.jQuery || document.write('<script src="js/vendor/jquery-1.10.1.min.js"><\/script>')</script>
  <script src="js/vendor/jquery.hashchange.min.js"></script>
  <script src="js/vendor/jquery.easytabs.min.js"></script>

  <script src="js/main.js"></script>

<style>
.alert {
    padding: 20px;
    background-color: #4CAF50;
    color: white;
    opacity: 1;
    transition: opacity 0.6s;
    margin-bottom: 15px;
}

.alert.success {background-color: #4CAF50;}
.alert.info {background-color: #2196F3;}
.alert.warning {background-color: #ff9800;}

.closebtn {
    margin-left: 15px;
    color: white;
    font-weight: bold;
    float: right;
    font-size: 22px;
    line-height: 20px;
    cursor: pointer;
    transition: 0.3s;
}

.closebtn:hover {
    color: black;
}
  
</style>



</head>
  <body class="bg-fixed bg-1">

   <div class="main-container">
    <div class="main wrapper clearfix">
      <!-- Header Start -->
        <header id="header">
            <div id="logo">
              
            </div>
        </header>
        <!-- Header End -->
        <!-- Main Tab Container -->
        <div id="tab-container" class="tab-container">
          <!-- Tab List -->
            <ul class='etabs'>
                <li class='tab active' id="tab-about">
                  <a href="/login"><i class="icon-user"></i><span> Sign In</span></a>
                </li>
                <li class='tab'>
                  <a href="/signup"><i class="icon-globe"></i><span>Sign Up</span></a>
                </li>
                
            </ul>
          <!-- End Tab List -->
            <div id="tab-data-wrap">

             
              </div>



              <!-- About Tab Data -->
                <div id="about">
                
                </div>
              <!-- End About Tab Data -->
              <!-- Portfolio Tab Data -->
              <div id="tab-data-wrap">
                <div id="portfolio">
                </section>

                <section class="clearfix">

                <h4 class="card col-xs-4" name="desc">Student Name</h4>
                <h4><%= name %></h4>
                <hr>
                
                <h4 class="card col-xs-4" name="desc">Portfolio Describtion</h4>
                <h4><%= tmpDesc %></h4>
                <hr>
                </section>

                <section class="clearfix">

                <% for(var i = 0 ; i < count ; i++){%>
                
                  <div class="g1">
                            <div class="image">
                                
                                <div class="card col-xs-4">
                                  <h4 class="card-title"><%= result[i].title %></h4>
                                  <img src="/images/work.jpg" alt="">
                                </div>

                                <div class="image-overlay">
                                    <div class="image-link">
                                        <a href="<%= result[i].link %>" class="btn">Visit</a>"
                                        
                                    </div>
                                </div>
                            </div>
                  </div>
                 
                  <% } %>

                </div>
                </div>
              <!-- End Portfolio Data -->
              <!-- Contact Tab Data -->
                <div id="contact">
                   
                </div>
              <!-- End Contact Data -->
            </div>


        <!-- End Tab Container -->
        <footer>
            <p>
                Media Enginnering And Technology Students' Portfolio System
            </p> 
        </footer>
    </div><!-- #main -->
</div><!-- #main-container -->



</body>
</html>

